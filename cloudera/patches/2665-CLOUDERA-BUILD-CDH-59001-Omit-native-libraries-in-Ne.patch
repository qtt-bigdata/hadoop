--- a/hadoop-hdfs-project/hadoop-hdfs/pom.xml
+++ b/hadoop-hdfs-project/hadoop-hdfs/pom.xml
@@ -586,6 +586,12 @@ http://maven.apache.org/xsd/maven-4.0.0.xsd">
                   <shadedPattern>com.cloudera.org.codehaus.jackson</shadedPattern>
                 </relocation>
               </relocations>
